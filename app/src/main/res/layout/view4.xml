<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <Button
        android:id="@+id/button3"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="296dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:background="#A33038"
        android:elevation="4dp"
        android:onClick="onButtonClicked"
        android:text="Next Page"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/pdf1"
        style="@android:style/Widget.Button"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/pdf"
        android:elevation="8dp"
        android:onClick="onClick"
        app:layout_constraintBottom_toTopOf="@+id/pdf3"
        app:layout_constraintHorizontal_bias="0.398"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/pdf2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.557"
        tools:layout_editor_absoluteY="99dp" />

    <Button
        android:id="@+id/pdf3"
        style="@android:style/Widget.Button"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginBottom="297dp"
        android:layout_marginLeft="101dp"
        android:background="@drawable/pdf"
        android:elevation="8dp"
        android:onClick="onButton"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/pdf2"
        style="@android:style/Widget.Button"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="88dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/pdf"
        android:elevation="8dp"
        android:onClick="onClicked"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBottom_toTopOf="@+id/pdf4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56" />

    <Button
        android:id="@+id/pdf4"
        style="@android:style/Widget.Button"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginBottom="298dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="70dp"
        android:background="@drawable/pdf"
        android:elevation="8dp"
        android:onClick="onButtonClick"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintLeft_toRightOf="@+id/pdf3"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="600dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="#A33038"
        android:text="Documents"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/pdf1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="-3dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="215dp"
        android:layout_height="26dp"
        android:text="CityWide Multi State Unit FDD.pdf"
        android:textColor="#000000"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/pdf1"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView9"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.355"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/pdf3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="158dp"
        android:layout_height="25dp"
        android:layout_marginTop="0dp"
        android:text="CityWide Education.PDF"
        android:textColor="#000000"
        app:layout_constraintTop_toBottomOf="@+id/pdf2"
        android:layout_marginRight="78dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/pdf4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="155dp"
        android:layout_height="24dp"
        android:layout_marginTop="0dp"
        android:text="CityWide Franchise.PDF"
        android:textColor="#000000"
        app:layout_constraintTop_toBottomOf="@+id/pdf3"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView11"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintHorizontal_bias="0.417"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="157dp"
        android:layout_height="24dp"
        android:layout_marginTop="0dp"
        android:text="Yearbook May2017.PDF"
        android:textColor="#000000"
        app:layout_constraintTop_toBottomOf="@+id/pdf4"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="79dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.0" />


</android.support.constraint.ConstraintLayout>